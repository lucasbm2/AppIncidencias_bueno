<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.MenuPrincipal">


    <Button
        android:id="@+id/botonUsuarios"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginTop="34dp"
        android:layout_marginBottom="32dp"
        android:text="Usuario"
        app:layout_constraintBottom_toTopOf="@+id/botonElemento"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/botonPrestamo"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="34dp"
        android:text="Prestamo"
        app:layout_constraintBottom_toTopOf="@+id/botonRoles"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonUbicacion" />

    <Button
        android:id="@+id/botonRoles"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="45dp"
        android:text="Roles"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonPrestamo" />

    <Button
        android:id="@+id/botonTipo"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="37dp"
        android:text="Tipo"
        app:layout_constraintBottom_toTopOf="@+id/botonUbicacion"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonIncidencia" />

    <Button
        android:id="@+id/botonSala"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="37dp"
        android:text="Sala"
        app:layout_constraintBottom_toTopOf="@+id/botonIncidencia"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonElemento" />

    <Button
        android:id="@+id/botonUbicacion"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="41dp"
        android:text="Ubicacion"
        app:layout_constraintBottom_toTopOf="@+id/botonPrestamo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonTipo" />

    <Button
        android:id="@+id/botonIncidencia"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="32dp"
        android:text="Incidencia"
        app:layout_constraintBottom_toTopOf="@+id/botonTipo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonSala" />

    <Button
        android:id="@+id/botonElemento"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_marginBottom="39dp"
        android:text="Elemento"
        app:layout_constraintBottom_toTopOf="@+id/botonSala"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/botonUsuarios" />

</androidx.constraintlayout.widget.ConstraintLayout>